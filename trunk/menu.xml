<script type="text/javascript">

	document.getElementById('home').onclick = function(){ Go('home');};
	document.getElementById('inicio').onclick = function() {Go('inicio');};
	document.getElementById('googleapps').onclick = function() {Go('googleapps');};
	document.getElementById('trucosyconsejos').onclick = function() {Go('trucosyconsejos');};
	document.getElementById('movilidad').onclick = function() {Go('movilidad');};
	document.getElementById('exito').onclick = function (){Go('exito');};
	
	function Go(destination)
	{
		if(destination=='home'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/home";}
		else if(destination=='inicio'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/iniciate";}
		else if(destination=='googleapps'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/google-apps";}
		else if(destination=='trucosyconsejos'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/trucosyconsejos";}
		else if(destination=='movilidad'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/movilidad";}
		else if(destination=='exito'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/casos-de-exito";}
		
		return false;
	}
	
	function redirect()
	{
		return false;
	}

	gadgets.util.registerOnLoadHandler(datospagina);
	
	function datospagina()
	{   
	   
		var parametros = gadgets.util.getUrlParameters()["parent"];
	
		if (parametros.indexOf("home")!=-1 || parametros == "https://sites.google.com/a/bbva.com/portaldeayuda/")
		{   			
			document.getElementsByClassName("homefirst")[0].style.backgroundColor = "rgb(0, 80, 152)";
			document.getElementsByClassName("homefirst")[0].style.boxShadow = "0 0 5px rgba(0, 0, 0, 0.5) inset";	
		}
		if(parametros.indexOf("casos-de-exito")!=-1){
			document.getElementsByClassName("exito")[0].style.backgroundColor = "rgb(0, 80, 152)";
			document.getElementsByClassName("exito")[0].style.boxShadow = "0 0 5px rgba(0, 0, 0, 0.5) inset";
		
		}
		if(parametros.indexOf("google-apps")!=-1){
			document.getElementsByClassName("apps")[0].style.backgroundColor = "rgb(0, 80, 152)";
			document.getElementsByClassName("apps")[0].style.boxShadow = "0 0 5px rgba(0, 0, 0, 0.5) inset";
		
		}
		if(parametros.indexOf("trucosyconsejos")!=-1){
			document.getElementsByClassName("trucos")[0].style.backgroundColor = "rgb(0, 80, 152)";
			document.getElementsByClassName("trucos")[0].style.boxShadow = "0 0 5px rgba(0, 0, 0, 0.5) inset";
		
		}
		if(parametros.indexOf("iniciate")!=-1){
			document.getElementsByClassName("iniciate")[0].style.backgroundColor = "rgb(0, 80, 152)";
			document.getElementsByClassName("iniciate")[0].style.boxShadow = "0 0 5px rgba(0, 0, 0, 0.5) inset";
		
		}
		if(parametros.indexOf("movilidad")!=-1){
			document.getElementsByClassName("movilidad")[0].style.backgroundColor = "rgb(0, 80, 152)";
			document.getElementsByClassName("movilidad")[0].style.boxShadow = "0 0 5px rgba(0, 0, 0, 0.5) inset";
		
		}
	}
	</script>

